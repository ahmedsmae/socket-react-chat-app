    "express-sslify": "^1.2.0",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.11",
    "multer": "^1.4.2",
    "request": "^2.88.0",
    "sharp": "^0.23.0",
    "socket.io": "^2.2.0"
